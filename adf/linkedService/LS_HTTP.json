{
	"name": "LS_HTTP",
	"properties": {
		"description": "Linked Service to get F1 data from Ergast",
		"parameters": {
			"base_url": {
				"type": "String",
				"defaultValue": "http://ergast.com/api/f1/"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{linkedService().base_url}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}